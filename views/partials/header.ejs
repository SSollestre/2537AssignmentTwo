<header class="p-3 text-bg-dark">
    <div class="container">
        <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">

            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                <li><a href="/" class="nav-link px-2 text-white">Home</a></li>
                <li><a href="/members" class="nav-link px-2 text-white">Profile</a></li>
            </ul>

            <% if (primaryUser===undefined) { %>
                <div class="text-end" style="outline: none">
                    <a class="btn btn-outline-light" href="/login" role="button">Login</a>
                    <a class="btn btn-warning" href="/signup" role="button">Sign Up</a>
                </div>
                <% } else {%>
                    <a class="btn btn-outline-danger" href="/logOut" role="button">Sign Out</a>
                    <% } %>

        </div>
    </div>
</header>